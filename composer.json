{
    "name": "germanow/yii2-active-record-seeder",
    "description": "Seeds database with test or default values.",
    "type": "library",
    "keywords": ["yii2","db", "seeding"],
    "homepage": "https://github.com/germanow/yii2-active-record-seeder",
    "require": {
        "yiisoft/yii2": "2.0.*",
        "la-haute-societe/yii2-save-relations-behavior": "^1.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Hermanov",
            "email": "ivan.hermanov@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "lhs\\Yii2SaveRelationsBehavior\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
